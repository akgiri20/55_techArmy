    height: 3em; /* Fixed height, assuming two lines max */
     overflow: hidden; /*Hide overflow beyond two lines */
    display: -webkit-box;
    -webkit-line-clamp: 2; /* Limit the content to two lines */
    -webkit-box-orient: vertical;
    box-orient: vertical;

    }
    .sticky-container {
    position: -webkit-sticky; /* For Safari */
    position: sticky;
    top: 0;
    z-index: 1000; /* Ensure it's above other content */
    display: flex;
    flex-direction: row;
    justify-content: flex-end;
    width:100%;

    }



    @media (max-width: 2000px) {
    .grid-container {
    grid-template-columns: repeat(2, 1fr); /* Change to 2 columns when the viewport width is 1200px or less */
    }
    }

    @media (max-width: 1800px) {
    .grid-container {
    grid-template-columns: 1fr; /* Change to 1 column when the viewport width is 800px or less */
    }

    .primary-news {
    width: 120%; /* Make the primary news take the full width */
    }

    .secondary-news {
    width: 100%; /* Adjust the secondary news width to be full width */
    }
    }

    @media (max-width: 1200px) {
    .news-box {
    flex-direction: column; /* Stack the primary and secondary news vertically */
    }

    .primary-news, .secondary-news {
    width: 100%; /* Make both primary and secondary news take up the full width */
    }

    .secondary-news {
    display: none; /* Hide the secondary news, showing only the primary news */
    }
    }
    @media (max-width: 800px) {
