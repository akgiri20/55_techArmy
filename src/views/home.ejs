    .primary-news-button {
    padding: 10px 20px;
    font-size: 1em;
    cursor: pointer;
    background-color: #000;
    color: #fff;
    border: none;
    border-radius: 5px;
    align-self: start; /* Align the button to the start (left) */
    margin-top: auto; /* Push the button to the bottom */
    }

    .primary-news-image-container {
    display: flex;
    flex-direction: column;
    justify-content: flex-start; /* Align the top of the image with the top of the content */
    flex-shrink: 0;
    padding-left: 20px;
    }

    .primary-news-image {
    border-radius: 10px;
    width: 400px;
    height: 280px;
    object-fit: cover;
    }
    .secondary-news {
    display: flex;
    flex-direction: column;
    width: 60%;
    margin-left: auto;
    margin-top: 12%;
    overflow-y: auto; /* Enable vertical scrolling */
    height: 305px;
    }

