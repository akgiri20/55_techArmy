    .loader {
      position: relative;
      -webkit-box-reflect: below -40px linear-gradient (transparent, rgba(241, 237, 237, 0.988));

    }

    .loader::before {

      content: "";
      position: absolute;
      top: 0;
      right: 0;
      width: 2px;
      height: 100%;
      background: #f2f3f4;
      animation: blinkCursor 1s steps(10) infinite;
    }

    .loader>h2::after {
      content: "";
      position: absolute;
      width: 70%;
      height: 20px;
      background-image: url(https://thisisdigital.co.uk/dist/images/underline-green.svg);
      bottom: -6%;
      left: 50%;
      z-index: 9;
      background-size: cover;
      transform: translate(-40%, 5%);

    }

    @keyframes blinkCursor {

      0%,
      10%,
      30%,
      100% {
        opacity: 0;
      }



    }

    .loader h2 {
      position: relative;
      color: rgb(164, 234, 255);
      letter-spacing: 10px;
      font-size: 9vw;
      text-transform: uppercase;
      animation: typing 8s steps(10) infinite;
      overflow: hidden;

    }

    @keyframes typing {

      0%,
      30%,
      90%,
      100% {
        width: 0;
      }

      30%,
      60% {
        width: 100%;
      }
    }

