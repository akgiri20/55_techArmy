    flex-shrink: 0;
    }

    .text-container {
    display: flex;
    flex-direction: column;
    justify-content: start;
    height: 100%;
    }

    .secondary-news-title {
    font-size: 1rem; /* Adjust font size as needed */
    font-family: 'Trebuchet MS', 'Lucida Sans Unicode', 'Lucida Grande', 'Lucida Sans', Arial, sans-serif;
    font-weight: lighter /* Make the title lighter */
    margin: 2px 0 0px 0; Adjust spacing as needed
    height: 2.8em;
    overflow: hidden; /* Hide overflow beyond two lines */
    display: -webkit-box;
    -webkit-line-clamp: 2; /* Limit the content to two lines */
    -webkit-box-orient: vertical;
    box-orient: vertical;
    
    }

    .secondary-news-subtitle {
    font-size: 0.8rem; /* Adjust font size as needed */
