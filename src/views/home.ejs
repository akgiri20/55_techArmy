    margin-bottom: 0px;
    text-decoration: none;
    /* Additional styling for the link */
    }

    .grid-container {
    display: grid;
    grid-template-columns: repeat(3, 1fr); /* 3 columns */
    grid-gap: 10px;
    padding: 0px; /* Adds some padding within the container */
    max-height: 280px; /* Example adjustment: total height - 'See All' link height - padding */
    overflow-y: auto; /* Allows scrolling within the grid if necessary */
    }

    .grid-item {
    display: flex;
    background-color: #fff;
    border-radius: 10px;
    padding: 1.5px;
    align-items: center;
    /* Ensure the height of each grid-item is consistent to make the layout predictable */
    }

    .secondary-news-image {
    border-radius: 10px; /* Match the border radius to your image */
    width: 120px; /* Adjust width as needed */
    height: 80px; /* Adjust height as needed */
