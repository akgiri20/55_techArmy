 <!DOCTYPE html>
 <html lang="en">

 <head>
   <meta charset="UTF-8">
   <meta http-equiv="X-UA-Compatible" content="IE=edge">
   <meta name="viewport" content="width=device-width, initial-scale=1.0">
   <title>TELEMED</title>
   <link rel="stylesheet" href="style.css">
   <!-- icon -->
   <script src="https://unpkg.com/ionicons@4.5.5/dist/ionicons.js"></script>
   <!-- GSAP  -->
   <script src="https://cdnjs.cloudflare.com/ajax/libs/gsap/2.1.2/TweenMax.min.js"></script>
 </head>

 <body>


   <div class="main-p">
     <div class="wrap">

       <!-- navbar================== -->
       <nav class="navbar">

         <div class="logo">TELEMED</div>
         <div class="categories">
           <ul>
             <li><a href="/home">HOME</a></li>
             <li><a href="/telepharm"> TELEPHARM</a></li>
             <li><a href="/teleconsult">TELECONSULT</a></li>
             <li><a href="/community">COMMUNITY</a></li>
             <li><a href="/about">ABOUT</a></li>

           </ul>
         </div>
         <div class="menu">
           <ion-icon name="ios-menu"></ion-icon>
         </div>
       </nav>


       <!-- red-bar=================== -->
       <div class="stripe"></div>
       <div class="stripe2"></div>
       <div class="coke">
         <img src="https://res.cloudinary.com/dtca2p99y/image/upload/v1708253169/istockphoto-1460830024-1024x1024-removebg-preview_lofgaj.png" class="coke-can">
       </div>

       <div class="small-coke">
         <li><img src="https://res.cloudinary.com/dtca2p99y/image/upload/v1708253330/med1-removebg-preview_tib6zl.png" onclick="imgSlider('coke-one.png');"></li>
         <!-- <li><img src="https://res.cloudinary.com/dtca2p99y/image/upload/v1708252251/coke-two_w6bpjk.png" onclick="imgSlider('coke-two.png');"></li> -->
       </div>

       <!-- text================= -->
       <div class="text">
         <h1>
           <span class="header" style="font-size: 5vw;"> HEALTH </span>
         </h1>
         <h2>CARE</h2>
         <p>
           <span class="para" style="font-size: 1.5vw;">
             at your door steps...
           </span>
         </p>
       </div>

       <div class="media">
         <ul>
           <li>
             <ion-icon name="logo-facebook"></ion-icon>
           </li>
           <li>
             <ion-icon name="logo-snapchat"></ion-icon>
           </li>
           <li>
             <ion-icon name="logo-instagram"></ion-icon>
           </li>
           <li>
             <ion-icon name="logo-youtube"></ion-icon>
           </li>
         </ul>
       </div>
     </div>

   </div>

   




   <script>
     function imgSlider(anything) {
       document.querySelector('.coke-can').src = anything;
     };

     TweenMax.from('.navbar', 1, {
       delay: 2,
       x: -40,
       opacity: 0,
       ease: Expo.easeInOut
     })

     TweenMax.to(".stripe", 2.4, {
       delay: 0,
       width: "65%",
       ease: Expo.easeInOut
     })

     TweenMax.from(".coke", 1, {
       delay: 1.4,
       opacity: 0,
       y: -1000,
       ease: Circ.easeInOut
     })

     TweenMax.from(".header", 2, {
       delay: 1.8,
       x: -700,
       ease: Circ.easeInOut
     })

     TweenMax.from(".text h2", 1, {
       delay: 3.3,
       opacity: 0,
       y: 40,
       ease: Circ.easeInOut
     })

     TweenMax.from(".text .para", 1.2, {
       delay: 3.9,
       opacity: 0,
       y: 40,
       ease: Circ.easeInOut
     })

     TweenMax.from(".small-coke", 1, {
       delay: 4.0,
       opacity: 0,
       y: 0,
       ease: Expo.easeInOut
     })

     TweenMax.from(".media ul li", 2, {
       delay: 5,
       opacity: 5,
       x: -40,
       ease: Circ.easeInOut
     })

     TweenMax.from(".stripe2", 2.5, {
       delay: 0,
       width: 5, // Animate width to make it appear left-to-right
       ease: Expo.easeInOut
     });
   </script>
 </body>


 <style>
   @import url('https://fonts.googleapis.com/css2?family=Josefin+Sans:wght@700&family=Poppins:ital,wght@0,200;0,600;0,900;1,900&display=swap');

