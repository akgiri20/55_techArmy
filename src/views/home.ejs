   }

   .navbar .menu {
     margin-right: -10px;
     font-size: 20px;
   }

   .stripe {
     position: absolute;
     height: 75px;
     /* width: 100%; */
     bottom: 45%;
     background: linear-gradient(to right, #f07c78, #c0342f);
   }

   .stripe2 {


     position: absolute;
     width: 100%;
     /* Adjust width as needed */
     height: 100%;
     /* Make stripe2 full height */
     top: 0;
     left: 10;
     /* Position stripe2 on the left side */
     background: linear-gradient(to right, #d39be5, #8a94ed);
     /* Left-to-right gradient */
     z-index: -1;
     /* Move stripe2 behind other content */
   }

   .coke {
     position: absolute;
     top: 50%;
     left: 30%;
     z-index: 1;
   }

   .coke img {
     transform: translate(-50%, -50%);
     max-height: 600px;
   }

   .small-coke {
     position: absolute;
     left: 85%;
     bottom: 70px;
     transform: translateX(-50%);
     display: flex;
     z-index: 1;
   }

   .small-coke li {
     list-style: none;
     display: inline-block;
     margin: 0 -77px;
     cursor: pointer;
     transition: 0.4s;
   }

   .small-coke li:hover {
     transform: translateY(-10px);
   }

   .small-coke li img {
     max-width: 200px;
   }

   .text {
     position: absolute;
     top: 120px;
     left: 500px;
   }

   .text h1 {
     font-size: 70px;
     text-transform: uppercase;
     font-weight: 900;
     color: #000000;
     margin-bottom: 25px;
     position: relative;
     overflow: hidden;
     height: 150px;
     width: 900px;
   }

   .text h1 .header {
     position: absolute;
     margin-left: 40px;
     bottom: 25px;
   }

   .text h2 {
     position: absolute;
     font-family: 'Josefin Sans', sans-serif;
     top: 100px;
     left: 80px;
     color: #f4f2ec;
     font-size: 100px;
     font-weight: 700;
   }

   .text p {
     width: 400px;
     font-size: 12px;
     margin-top: 45px;
     margin-left: 50px;
     position: relative;
     height: 110px;
     color: #000000;
     font-weight: 600;
   }

   .media {
     position: absolute;
     bottom: 210px;
     left: 40px;
   }

   .media ul {
     position: absolute;
     list-style: none;
   }

   .logo {
     position: relative;
     /* Position cococola logo relative to its container */
     z-index: 1;
     /* Ensure the logo stays above stripe2 */
     font-size: 20;
   }


   .media ul li {
     color: #fefafa;
     padding: 12px 0;
     size: 5px;
   }
 </style>

 </html>